            "error_code":{"type":"int" },
            "errors":{"type":"str" },
            "coolant_in":{"units":"temp"},
            "coolant_out":{"units":"temp"},
            "oil_temp":{"units":"temp"},
            "helium_temp":{"units":"temp"},
            "low_pressure":{"units":"press"},
            "low_pressure_average":{"units":"press"},
            "high_pressure":{"units":"press"},
            "high_pressure_average":{"units":"press"},
            "delta_pressure_average":{"units":"press"},
