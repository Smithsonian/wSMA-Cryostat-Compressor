{
    "logging_interval":30,
    "smax_config":{
        "smax_table":"cryostat",
        "smax_key":"compressor",
        "smax_power_control_key":"compressor_control",
        "smax_inverter_freq_control_key":"frequency_control"
    },
    "serial_server":"192.168.1.13",
    "compressor":{
        "ip_address":"192.168.1.12",
        "port":502,
        "logged_data":{
            "enabled":{"type":"int" },
            "state_code":{"type":"int" },
